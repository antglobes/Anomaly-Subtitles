	<string id="characters_voice_human_freedom_1_fight_cover_fire_cover_fire_1">
        <text>Давай, Мэн, я тебя берегу!</text>
    </string>
	<string id="characters_voice_human_freedom_1_fight_cover_fire_cover_fire_2">
